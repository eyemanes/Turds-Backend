{
  "name": "turds-nation-backend",
  "version": "2.0.0",
  "description": "TURDS Nation Backend API - Secured",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "build": "npm run security:check && echo 'Build complete'",
    "dev": "nodemon server-secure.js",
    "start": "node server-secure.js",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "test": "jest --coverage",
    "test:security": "npm run security:check && npm run test"
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@vercel/node": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  }
}
